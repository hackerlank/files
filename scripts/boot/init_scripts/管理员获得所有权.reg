Windows Registry Editor Version 5.00
[HKEY_CLASSES_ROOT\*\shell\runas]
@="管理员取得所有权(&G)"
"NoWorkingDirectory"=""
[HKEY_CLASSES_ROOT\*\shell\runas\command]
@="cmd.exe /c takeown /f \"%1\" && icacls \"%1\" /reset /C && PAUSE"
"IsolatedCommand"="cmd.exe /c takeown /f \"%1\" && icacls \"%1\" /reset /C && PAUSE"
